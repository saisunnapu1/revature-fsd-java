package Strings;

public class MethodObject {
	int sum=0;
	public static void main(String[] args) {
		Student s=new Student();
		School i=new School();
		MethodObject m= new MethodObject();
		m.display(i);
		}
     void  display(School c) {
    	 int sum=c.tel+c.eng+c.math+c.soc;
    	 System.out.println(sum);
    	 
      }
	

	
}




