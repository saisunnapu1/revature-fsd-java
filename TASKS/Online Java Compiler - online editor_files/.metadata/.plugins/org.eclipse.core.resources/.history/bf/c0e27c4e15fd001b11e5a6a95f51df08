package Types;

public class All {
	int a=33,b=2;
void addx() {
	int c=a+b;
	System.out.println(c);
}
}class add extends eg2{
void add() {
	System.out.println(a+b);
}
}class sub extends add{
void sub() {
System.out.println(a-b);
	
}
}class div extends sub {
void div() {
	System.out.println(a/b);
}
}class inheritance extends All{
public static void main(String[] args) {
	All k=new All();
	k.addx();


}

}
