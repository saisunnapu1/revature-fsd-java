package Constructor;

public class College {
	int Roll;
	String name;
	 static String college="Anits";
	

	public static void main(String[] args) {
		System.out.println("BANK NAME---UNION BANK");
		System.out.println("ACCOUNT NUMBER---100510100096660")
		int balance=5000;
		System.out.println("----------------------");
	    System.out.println("Roll   "+"name   "+"college   ");
	    System.out.println("----------------------");
	    College.change();
		College s1=new College(1,"a");
		College s2=new College(2,"b");
		College s3=new College(3,"c");
		College s4=new College(4,"d");
		College s5=new College(5,"e");
		System.out.println("----------------------");
		
	    
	}
	 void change() {
		college="bavadhyo";
	}
	College (int x,String y){
		    Roll=x;
		    name=y;
		    System.out.println(+Roll+"     "+name+"         "+college);
		    System.out.println(balance);
	
	
	}

}
