package Constructor;

public class This {
	int x;//instance variable
	String name="sai";
	void display() {
		//int x=20;
		String name="kumar";
		System.out.println(this.name);
		System.out.println(x);
		
		
	}
	This(int y){
		x=y;
		System.out.println(y);
	}
	
	public static void main(String[] args) {
		This s1=new This(55);
		//This s2=new This();
		s1.display();
		//System.out.println();
		
	}

}
