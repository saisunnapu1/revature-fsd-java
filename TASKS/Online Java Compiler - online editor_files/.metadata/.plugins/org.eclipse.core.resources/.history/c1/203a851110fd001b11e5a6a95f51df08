package Types;

public class eg1{
	float salary=40000;
}
   class programmer extends eg1{
	   int bonus=1000;
	   public static void main(String[] args) {
		   programmer s=new programmer();
		   System.out.println("total salary"+s.salary+s.bonus);
			//System.out.println(salary);
		}

	   
   }
   
