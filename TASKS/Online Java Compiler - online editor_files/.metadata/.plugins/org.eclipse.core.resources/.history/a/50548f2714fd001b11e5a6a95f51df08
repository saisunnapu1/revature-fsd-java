package Types;

public class eg2 {
	int a=33,b=2;

}class add extends eg2{
	void add() {
		System.out.println(a+b);
	}
}class sub extends add{
	void sub() {
	System.out.println(a-b);
		
}
}class div extends sub {
	void div() {
		System.out.println(a/b);
	}
	/*void mul() {
		System.out.println(a*b);
	}*/
}class inheritance{
	public static void main(String[] args) {
		div k=new div();
		k.add();
		k.sub();
		k.div();
		

	}
}
