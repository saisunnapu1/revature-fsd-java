package Dates;

import java.time.LocalDate;
import java.time.LocalTime;

public class eg2 {

	public static void main(String[] args) {
		 LocalDate date1 = LocalDate.now();
 		System.out.println(date1);
 		 LocalTime time1 = LocalTime.now();
 		System.out.println(date1);
 		System.out.println(time1);



	}

}
